          <a class="btn btn-primary btn-xl js-scroll-trigger" href="{{path('verification')}}">Obtenir le statut d'un coupon dès maintenant</a>
        </div>
      </div>
    </div>
  </header>

  

  <!-- About Section -->
  <section class="page-section bg-primary" id="about">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-8 text-center">
